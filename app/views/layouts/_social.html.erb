<div class="sharing-widgets">
  <script type="text/javascript" src="//platform.twitter.com/widgets.js"></script>
  <script type="text/javascript">
     (function(d, s, id) {
         var js, fjs = d.getElementsByTagName(s)[0];
         if (d.getElementById(id)) {return;}
         js = d.createElement(s); js.id = id;
         js.src = "//connect.facebook.net/pl_PL/all.js#xfbml=1";
         fjs.parentNode.insertBefore(js, fjs);
     }(document, 'script', 'facebook-jssdk'));
  </script>
  <script type="text/javascript">
    (function() {
      var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
      po.src = 'https://apis.google.com/js/plusone.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
    })();
  </script>
  <div id="fb-root"></div>
  <div class="widget">
     <div class="g-plusone" data-size="medium"></div>
  </div>
  <div class="widget">
      <a href="https://twitter.com/share" class="twitter-share-button" data-url="<%=HOST %>"
        data-text="Gmin Plus to projekt nadzorowania wydawania podatkÃ³w w gminach">Tweet</a>
  </div>
  <div class="widget">
      <div class="fb-like" data-href="<%= HOST %>" data-send="false" data-layout="button_count"
           data-width="55" data-show-faces="false" data-font="arial">
      </div>
  </div>
</div>
